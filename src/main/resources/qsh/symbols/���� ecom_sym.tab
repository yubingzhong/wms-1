SYMBOL 
  NAME "dashed-line-short"
  TYPE ELLIPSE
  FILLED TRUE
  POINTS 1 1 END
  STYLE 5 5 END
END

# --------------------

SYMBOL 
  NAME "dashed-line-long"
  TYPE ELLIPSE
  FILLED TRUE
  POINTS 1 1 END
  STYLE 10 10 END
END

# --------------------

SYMBOL 
  NAME "dash-dot"
  TYPE ELLIPSE
  FILLED TRUE
  POINTS 1 1 END
  STYLE 20 6 2 6 END
END

# --------------------

SYMBOL 
  NAME "dash-dot-dot"
  TYPE ELLIPSE
  FILLED TRUE
  POINTS 1 1 END
